{
  "name": "react-clear-cache-v2",
  "version": "2.0.0",
  "description": "A component to manage application updates.",
  "author": "hckhanh",
  "license": "MIT",
  "repository": "hckhanh/react-clear-cache",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "bin": {
    "generate-build-meta": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "release:major": "npm version major && git push origin && git push origin --tags",
    "release:minor": "npm version minor && git push origin && git push origin --tags",
    "release:patch": "npm version patch && git push origin && git push origin --tags"
  },
  "dependencies": {
    "minimist": "1.2.6",
    "use-persisted-state": "0.3.0",
    "uuid": "3.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@rollup/plugin-url": "7.0.0",
    "@svgr/rollup": "6.2.1",
    "@types/jest": "^28.1.3",
    "@types/prettier": "^2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/use-persisted-state": "^0.3.1",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cross-env": "7.0.3",
    "gh-pages": "4.0.0",
    "prettier": "2.7.1",
    "react-scripts": "5.0.1",
    "rollup": "2.75.7",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "1.6.2",
    "rollup-plugin-typescript2": "0.17.2",
    "rollup-plugin-url": "3.0.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1"
}
